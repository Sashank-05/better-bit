{
    "version": 2,
    "builds": [
        {
            "src": "app/__init__.py",
            "use": "@vercel/python"
        },
        {
            "src": "app/static/images/*.png",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}